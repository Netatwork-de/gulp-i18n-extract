{
	"compilerOptions": {
		"target": "es6",
		"module": "es6",
		"isolatedModules": false,
		"jsx": "react",
		"declaration": false,
		"noImplicitAny": false,
		"removeComments": true,
		"noLib": false,
		"preserveConstEnums": true,
		"suppressImplicitAnyIndexErrors": false,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"experimentalAsyncFunctions": true,
    "allowSyntheticDefaultImports": true      
	}
}
